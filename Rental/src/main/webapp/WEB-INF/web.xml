<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

  <welcome-file-list>
    <welcome-file>Rental.jsp</welcome-file>
  </welcome-file-list>
  
   <servlet>
    <servlet-name>RentalServlet</servlet-name>
    <servlet-class>com.rental.RentalServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RentalServlet</servlet-name>
    <url-pattern>/RentalServlet</url-pattern>
</servlet-mapping>

   <servlet>
    <servlet-name>FormAction</servlet-name>
    <servlet-class>com.rental.FormAction</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>FormAction</servlet-name>
    <url-pattern>/FormAction</url-pattern>
</servlet-mapping>

   <servlet>
    <servlet-name>UpdateRentServlet</servlet-name>
    <servlet-class>com.rental.UpdateRentServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>UpdateRentServlet</servlet-name>
    <url-pattern>/UpdateRentServlet</url-pattern>
</servlet-mapping>

</web-app>